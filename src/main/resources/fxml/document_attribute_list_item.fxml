<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import np.com.ngopal.control.AutoFillTextBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>
<?import java.lang.String?>
<?import java.net.URL?>
<AnchorPane id="DocumentAttributeListItemUI" minHeight="60.0" prefHeight="60.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="digitalbedrock.software.pbcore.controllers.DocumentAttributeItemController">
   <stylesheets>
      <URL value="@../styles/style.css" />
   </stylesheets>
   <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <columnConstraints>
         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         <ColumnConstraints halignment="CENTER" hgrow="NEVER" maxWidth="25.0" minWidth="25.0" prefWidth="100.0" />
      </columnConstraints>
      <rowConstraints>
         <RowConstraints maxHeight="25.0" minHeight="25.0" prefHeight="25.0" valignment="CENTER" vgrow="NEVER" />
         <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
      </rowConstraints>
      <padding>
         <Insets top="5.0" />
      </padding>
      <Label fx:id="attributeNameLbl" styleClass="formLabel" text="Label" />
      <Button fx:id="removeButton" mnemonicParsing="false" GridPane.columnIndex="1">
         <graphic>
            <FontIcon iconLiteral="mdi-close" iconSize="17" />
         </graphic>
      </Button>
      <HBox GridPane.rowIndex="1">
         <AutoFillTextBox fx:id="autoCompleteTF" maxWidth="1.7976931348623157E308" styleClass="attributeValueTextBox" GridPane.rowIndex="1" HBox.hgrow="ALWAYS" />
         <Button fx:id="btnSelectCV" mnemonicParsing="false" onAction="#selectCV" GridPane.columnIndex="1">
            <graphic>
               <FontIcon iconLiteral="mdi-magnify" />
            </graphic>
            <styleClass>
               <String fx:value="listActionButton" />
               <String fx:value="dimmedIcon" />
            </styleClass>
         </Button>
      </HBox>

      <FontIcon fx:id="valueMissingIcon" iconLiteral="mdi-alert" iconSize="17" styleClass="warningIcon" GridPane.columnIndex="1" GridPane.rowIndex="1" />
   </GridPane>
</AnchorPane>
